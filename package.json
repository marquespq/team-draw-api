{
  "name": "team-draw-node-typescript-api",
  "version": "1.0.0",
  "description": "Team Draw NodeJs API com Typescript",
  "projectPrefix": "Team Draw",
  "main": "index.js",
  "engines": {
    "npm": ">=6.0.0",
    "node": ">=14.0.0"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc",
    "dev": "ts-node-dev --transpile-only --respawn --inspect=9229 src/index.ts",
    "test": "cross-env NODE_ENV=test DOTENV_CONFIG_PATH=.env.test jest --detectOpenHandles --runInBand",
    "test:unit": "cross-env NODE_ENV=test DOTENV_CONFIG_PATH=.env.test jest -c jest-unit.config.js --detectOpenHandles",
    "test:review": "jest --detectOpenHandles --runInBand",
    "test:unit:review": "jest -c jest-unit.config.js --detectOpenHandles",
    "lint": "eslint . --ext ts",
    "lint:fix": "eslint . --ext ts --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marquespq/team-draw-api"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marquespq/team-draw-api"
  },
  "homepage": "https://github.com/marquespq/team-draw-api",
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "commitlint-plugin-function-rules": "^1.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "firebase-admin": "^10.0.0",
    "flat": "^5.0.2",
    "helmet": "^4.6.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mixpanel": "^0.14.0",
    "mongoose": "^6.0.13",
    "morgan": "^1.10.0",
    "nanoid": "^3.1.30",
    "pagarme": "^4.20.0",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.10",
    "winston": "^3.3.3",
    "yup": "^0.32.11"
  }
}
